{
  "name": "expense-bot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage --verbose",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:ci": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage --ci --reporters=default --reporters=jest-junit --coverageReporters=text --coverageReporters=lcov --coverageReporters=html",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=tests/unit",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/jest": "^29.5.5",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "supertest": "^6.3.3"
  }
}
